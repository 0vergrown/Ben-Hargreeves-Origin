{
	"name":"Physical Body",
	"description":"You are able to become a physical body until your cooldown runs out. [Primary]",
	"badges":[
	   {
		  "type":"origins:tooltip",
		  "sprite":"origins:textures/gui/badge/toggle.png",
		  "text":[
			 "Toggleable with [",
			 {
				"keybind":"key.origins.primary_active"
			 },
			 "] key."
		  ]
	   },
	   {
		  "type":"origins:tooltip",
		  "sprite":"origins:textures/gui/badge/info.png",
		  "text":"You can be a physical person but a bar slowly drains when you are in that state."
	   }
	],
	"type":"origins:multiple",
	"form":{
	   "type":"origins:resource",
	   "min":0,
	   "max":1,
	   "hud_render":{
		  "should_render":false
	   },
	   "start_value":0
	},
	"timer":{
	   "type":"origins:resource",
	   "min":0,
	   "max":300,
	   "hud_render":{
		  "should_render":true,
		  "sprite_location":"origins:textures/gui/community/spiderkolo/resource_bar_03.png",
		  "bar_index":9
	   },
	   "start_value":0,
	   "max_action":{
		  "type":"origins:change_resource",
		  "resource":"ben_hargreeves:physical_body_form",
		  "change":0,
		  "operation":"set"
	   }
	},
	"soul_body":{
	   "type":"origins:model_color",
	   "red":1.0,
	   "green":1.0,
	   "blue":1.0,
	   "alpha":0.4,
	   "condition":{
		  "type":"origins:resource",
		  "resource":"ben_hargreeves:physical_body_form",
		  "comparison":"==",
		  "compare_to":0
	   }
	},
	"active":{
	   "type":"origins:active_self",
	   "entity_action":{
		  "type":"origins:if_else",
		  "condition":{
			 "type":"origins:resource",
			 "resource":"ben_hargreeves:physical_body_form",
			 "comparison":"==",
			 "compare_to":0
		  },
		  "if_action":{
			 "type":"origins:and",
			 "actions":[
				{
				   "type":"origins:change_resource",
				   "resource":"ben_hargreeves:physical_body_form",
				   "change":1,
				   "operation":"set"
				},
				{
				   "type":"origins:play_sound",
				   "sound":"minecraft:entity.elder_guardian.ambient",
				   "volume":1.00,
				   "pitch":0.59
				}
			 ]
		  },
		  "else_action":{
			 "type":"origins:and",
			 "actions":[
				{
				   "type":"origins:change_resource",
				   "resource":"ben_hargreeves:physical_body_form",
				   "change":0,
				   "operation":"set"
				},
				{
				   "type":"origins:play_sound",
				   "sound":"minecraft:entity.elder_guardian.ambient",
				   "volume":1.00,
				   "pitch":0.59
				}
			 ]
		  }
	   },
	   "cooldown":200,
	   "hud_render":{
		  "should_render":true,
		  "sprite_location":"origins:textures/gui/community/huang/resource_bar_01.png",
		  "bar_index":5
	   },
	   "key":{
		  "key":"key.origins.primary_active",
		  "continuous":false
	   }
	},
	"overlay_when_alive":{
	   "type":"origins:overlay",
	   "texture":"minecraft:textures/misc/nausea.png",
	   "strength":0.25,
	   "red":0.53725490196,
	   "green":0.69803921568,
	   "blue":0.96078431372,
	   "draw_mode":"nausea",
	   "draw_phase":"below_hud",
	   "hide_with_hud":false,
	   "visible_in_third_person":false,
	   "condition":{
		  "type":"origins:resource",
		  "resource":"ben_hargreeves:physical_body_form",
		  "comparison":"==",
		  "compare_to":1
	   }
	},
	"phase":{
	   "type":"origins:phasing",
	   "blacklist":true,
	   "block_condition":{
		  "type":"origins:in_tag",
		  "tag":"origins:unphasable"
	   },
	   "render_type":"blindness",
	   "view_distance":10,
	   "phase_down_condition":{
		  "type":"origins:and",
		  "conditions":[
			 {
				"type":"origins:sneaking"
			 },
			 {
				"type":"origins:on_block"
			 }
		  ]
	   },
	   "condition":{
		  "type":"origins:resource",
		  "resource":"ben_hargreeves:physical_body_form",
		  "comparison":"==",
		  "compare_to":0
	   }
	},
	"timer_gain_and_drain":{
	   "type":"origins:action_over_time",
	   "entity_action":{
		  "type":"origins:if_else",
		  "condition":{
			 "type":"origins:resource",
			 "resource":"ben_hargreeves:physical_body_form",
			 "comparison":"==",
			 "compare_to":1
		  },
		  "if_action":{
			 "type":"origins:change_resource",
			 "resource":"ben_hargreeves:physical_body_timer",
			 "change":1,
			 "operation":"add"
		  },
		  "else_action":{
			 "type":"origins:change_resource",
			 "resource":"ben_hargreeves:physical_body_timer",
			 "change":-1,
			 "operation":"add"
		  }
	   },
	   "interval":20
	},
	"death_at_timer_max":{
	   "type":"origins:action_over_time",
	   "rising_action":{
		  "type":"apoli:execute_command",
		  "command":"kill @s"
	   },
	   "interval":1,
	   "condition":{
		  "type":"origins:resource",
		  "resource":"ben_hargreeves:physical_body_timer",
		  "comparison":"==",
		  "compare_to":300
	   }
	}
}